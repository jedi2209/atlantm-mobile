{
  "name": "Atlant-M",
  "version": "6.4.1",
  "private": true,
  "scripts": {
    "build": "bundle exec fastlane ios localNoCompile; npm run android-build;",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "npx patch-package; npx pod-install; npx jetify;",
    "react": "npx react-native start --reset-cache",
    "iphone": "npx react-native run-ios --simulator=\"iPhone SE\"",
    "iphonePro": "npx react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "iphoneProMax": "npx react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios-build": "bundle exec fastlane ios beta; rm atlantm.app.dSYM.zip atlantm.ipa;",
    "android": "cd android && ./gradlew clean; cd ../; open -a iTerm ./android/dev.sh && adb wait-for-device && sleep 10; adb uninstall com.atlantm; sleep 10; npx react-native run-android",
    "android-local": "bundle exec fastlane android test && adb uninstall com.atlantm; adb install ./android/app/build/outputs/apk/release/app-arm64-v8a-release.apk && adb shell am start -n com.atlantm/.MainActivity",
    "android-release": "git checkout ./android/app/versioning/app_version.properties && bundle exec fastlane android release && open ./android/app/build/outputs/apk/release;",
    "android-build": "bundle exec fastlane android internal",
    "reinstall": "./reinstall.sh",
    "cleanTmp": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf ios/build/ android/app/build/ android/build/ ios/rbtse.xcodeproj/xcuserdata/",
    "clean": "npx react-native-clean-project",
    "version": "./version.sh"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^1.1.2",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/async-storage": "1.12.0",
    "@react-native-community/datetimepicker": "^3.0.2",
    "@react-native-community/google-signin": "^4.0.3",
    "@react-native-community/picker": "^1.6.6",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-community/viewpager": "^4.1.6",
    "@react-navigation/native": "^3.7.13",
    "@sentry/react-native": "^1.7.0",
    "acorn": "^8.0.1",
