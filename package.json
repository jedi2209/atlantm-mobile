{
  "name": "Atlant-M",
  "version": "6.0.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "cd ./ios; pod update; pod install; cd ../; npx jetify;",
    "iphone": "react-native run-ios --simulator=\"iPhone 11\"",
    "iphonePro": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "iphoneProMax": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "react": "react-native start --reset-cache",
    "ios-build": "bundle exec fastlane ios beta",
    "android": "cd android && ./gradlew clean; cd ../; adb uninstall com.atlantm; react-native run-android",
    "android-test": "bundle exec fastlane android test && adb uninstall com.atlantm; adb install ./android/app/build/outputs/apk/release/app-release.apk; adb shell am start -n com.atlantm/.MainActivity",
    "android-release": "git checkout ./android/app/versioning/app_version.properties && bundle exec fastlane android release && open ./android/app/build/outputs/apk/release;",
    "android-build": "bundle exec fastlane android internal",
    "reinstall": "./reinstall.sh",
    "clean_old": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf ios/build/; rm -rf android/build/; rm -rf ios/rbtse.xcodeproj/xcuserdata/",
    "clean": "react-native-clean-project",
    "version": "./version.sh"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@ptomasroos/react-native-multi-slider": "^1.0.0",
    "@react-native-community/async-storage": "^1.6.3",
    "@react-native-community/google-signin": "^3.0.3",
    "@react-native-community/slider": "^2.0.7",
    "@react-native-community/viewpager": "^3.3.0",
    "@sentry/react-native": "^1.1.0",
    "acorn": "^7.1.0",
    "amplitude-js": "5.8.0",
    "html-entities": "^1.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-native": "0.61.5",
    "react-native-actionsheet": "^2.4.2",
    "react-native-animatable": "^1.3.3",
    "react-native-clean-project": "^3.3.0",
    "react-native-communications": "^2.2.1",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^5.3.1",
    "react-native-devsettings-android": "^0.3.0",
    "react-native-fbsdk": "^1.1.1",
    "react-native-gallery-swiper": "^1.23.1",
    "react-native-gesture-handler": "^1.5.2",
    "react-native-image-crop-picker": "0.26.1",
    "react-native-image-gallery": "^2.1.5",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-maps": "0.26.1",
    "react-native-onesignal": "^3.5.0",
    "react-native-rate": "^1.1.10",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^2.0.0-alpha.15",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swiper": "^1.6.0-rc.3",
    "react-native-webview": "^7.5.2",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "1.10.3",
    "react-navigation-tabs": "2.6.2",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-form": "7.3.0",
    "redux-persist": "4.10.2",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "^0.11.2",
    "rn-range-slider": "^1.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-check-constants": "*",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "*",
    "jest": "^24.9.0",
    "jetifier": "^1.6.5",
    "metro": "^0.57.0",
    "metro-core": "^0.57.0",
    "metro-react-native-babel-preset": "^0.57.0",
    "native-base": "^2.13.8",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-svg-asset-plugin": "^0.3.0",
    "react-test-renderer": "^16.12.0",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
