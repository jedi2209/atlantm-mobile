{
  "name": "Atlant-M",
  "version": "6.1.0",
  "private": true,
  "scripts": {
    "build": "npm run ios-build; npm run android-build;",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "cd ./ios; pod update; pod install; cd ../; npx jetify;",
    "react": "npx react-native start --reset-cache",
    "iphone": "npx react-native run-ios --simulator=\"iPhone SE\"",
    "iphonePro": "npx react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "iphoneProMax": "npx react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios-build": "bundle exec fastlane ios beta; rm atlantm.app.dSYM.zip atlantm.ipa;",
    "android": "cd android && ./gradlew clean; cd ../; open -a iTerm ./android/dev.sh & adb wait-for-device; sleep 10; adb uninstall com.atlantm; npx react-native run-android",
    "android-local": "bundle exec fastlane android test && adb uninstall com.atlantm; adb install ./android/app/build/outputs/apk/release/app-release.apk; adb shell am start -n com.atlantm/.MainActivity",
    "android-release": "git checkout ./android/app/versioning/app_version.properties && bundle exec fastlane android release && open ./android/app/build/outputs/apk/release;",
    "android-build": "bundle exec fastlane android internal",
    "reinstall": "./reinstall.sh",
    "clean_old": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf ios/build/; rm -rf android/build/; rm -rf ios/rbtse.xcodeproj/xcuserdata/",
    "clean": "npx react-native-clean-project",
    "version": "./version.sh"
  },
  "dependencies": {
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/async-storage": "1.4.2",
    "@react-native-community/google-signin": "^4.0.0",
    "@react-native-community/slider": "^2.0.9",
    "@react-native-community/viewpager": "^3.3.0",
    "@react-navigation/native": "^3.6.2",
    "@sentry/react-native": "^1.3.7",
    "acorn": "^7.1.1",
    "amplitude-js": "^6.0.1",
    "color": "^3.1.2",
    "html-entities": "^1.3.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-actionsheet": "^2.4.2",
    "react-native-animatable": "^1.3.3",
    "react-native-awesome-alerts": "^1.3.2",
    "react-native-clean-project": "^3.3.0",
    "react-native-communications": "^2.2.1",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^5.5.4",
    "react-native-fbsdk": "^1.1.2",
    "react-native-gallery-swiper": "^1.25.3",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-crop-picker": "0.28.0",
    "react-native-image-gallery": "^2.1.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-map-link": "^2.7.8",
    "react-native-maps": "0.27.1",
    "react-native-modal": "^11.5.6",
    "react-native-onesignal": "^3.7.2",
    "react-native-phone-input": "^0.2.4",
    "react-native-rate": "^1.2.1",
    "react-native-reanimated": "^1.8.0",
    "react-native-restart": "0.0.14",
    "react-native-screens": "^2.4.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-swiper": "^1.6.0",
    "react-native-tab-navigator": "^0.3.4",
    "react-native-vkontakte-login": "^1.0.0",
    "react-native-webview": "^9.2.2",
    "react-navigation": "^3.12.0-alpha.0",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "1.10.3",
    "react-navigation-tabs": "2.6.2",
    "react-redux": "5.1.2",
    "redux": "4.0.5",
    "redux-form": "8.3.1",
    "redux-persist": "4.10.2",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "^0.12.0",
    "sharp": "^0.25.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-check-constants": "*",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^25.4.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^24.9.0",
    "jetifier": "^1.6.5",
    "metro-react-native-babel-preset": "^0.59.0",
    "native-base": "^2.13.12",
    "react-native-snap-carousel": "^3.9.0",
    "react-native-svg-asset-plugin": "^0.3.0",
    "react-test-renderer": "^16.13.1",
    "reactotron-react-native": "^5.0.0",
    "redux-logger": "^3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
