{
  "name": "atlantm",
  "version": "5.2.0",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "iphonex": "react-native run-ios --simulator=\"iPhone X\"",
    "ios-build": "bundle exec fastlane ios beta",
    "android": "adb uninstall com.atlantm; react-native run-android",
    "android-test": "bundle exec fastlane android test && adb uninstall com.atlantm; adb install android/app/build/outputs/apk/release/app-release.apk; adb shell am start -n com.atlantm/.MainActivity",
    "android-build": "bundle exec fastlane android internal",
    "reinstall": "./reinstall.sh",
    "clean": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf ios/build/; rm -rf android/build/; rm -rf ios/rbtse.xcodeproj/xcuserdata/",
    "version": "./version.sh"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "moment": "2.22.1",
    "native-base": "2.8.1",
    "prop-types": "15.6.1",
    "react": "16.5.0",
    "react-native": "0.57.2",
    "react-native-actionsheet": "2.4.2",
    "react-native-amplitude-analytics": "0.2.3",
    "react-native-animatable": "1.3.0",
    "react-native-communications": "2.2.1",
    "react-native-datepicker": "1.7.2",
    "react-native-device-info": "0.24.0",
    "react-native-image-crop-picker": "0.21.2",
    "react-native-image-gallery": "git+https://github.com/tavriaforever/react-native-image-gallery.git",
    "react-native-loading-spinner-overlay": "0.5.2",
    "react-native-maps": "0.21.0",
    "react-native-onesignal": "^3.2.11",
    "react-native-rate": "^1.1.6",
    "react-native-sentry": "0.39.1",
    "react-native-splash-screen": "3.1.1",
    "react-native-swiper": "1.5.13",
    "react-navigation": "1.1.2",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-form": "7.3.0",
    "redux-persist": "4.10.2",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "0.10.13"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "babel-plugin-module-resolver": "3.1.1",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.48.0",
    "react-test-renderer": "16.5.0",
    "redux-logger": "3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
