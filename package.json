{
	"name": "Atlant-M",
	"version": "10.7.0",
	"private": true,
	"scripts": {
		"build": "yarn ios-build; yarn android-build;",
		"build:ci": "yarn ios-version; yarn android-version;",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"preinstall": "yarn config set ignore-engines true",
		"postinstall": "bash ./postinstall.sh",
		"pod-update": "cd ios; bundle exec pod update; bundle exec pod install; cd ../",
		"react": "watchman watch-del $(pwd); watchman watch-project $(pwd); npx react-native start --reset-cache",
		"clean:android": "cd android && ./gradlew clean && cd ../",
		"clean:android:win": "cd ./android; ./gradlew clean; cd ../",
		"clean:ios": "xattr -w com.apple.xcode.CreatedByBuildSystem true $(pwd)/ios/build; cd ios && xcodebuild clean && cd ../",
		"iphone": "yarn react-native run-ios --simulator=\"iPhone 15\"",
		"iphonePro": "yarn react-native run-ios --simulator=\"iPhone 15 Pro\"",
		"iphoneProMax": "yarn react-native run-ios --simulator=\"iPhone 15 Pro Max\"",
		"iphoneProMax13": "yarn react-native run-ios --simulator=\"iPhone 13 Pro Max\"",
		"ios13": "yarn iphoneProMax13",
		"ios": "yarn iphoneProMax",
		"ios-build": "bundle exec fastlane ios beta; yarn bugsnag:upload-dsym; rm atlantm.app.dSYM.zip atlantm.ipa;",
		"ios-version": "bundle exec fastlane ios version",
		"android": "yarn clean:android; open -a iTerm ./android/dev.sh && adb wait-for-device && sleep 10; adb uninstall com.atlantm; sleep 10; npx react-native run-android --active-arch-only",
		"android:mac": "yarn android",
		"android:win": "yarn clean:android:win; ./android/dev.ps1; adb wait-for-device; sleep 10; adb uninstall com.atlantm; sleep 10; npx react-native run-android --active-arch-only",
		"android-local": "yarn clean:android; bundle exec fastlane android test && adb uninstall com.atlantm; adb install ./android/app/build/outputs/bundle/release/app-release.aab && adb shell am start -n com.atlantm/.MainActivity",
		"android-release": "yarn clean:android; git checkout ./android/app/versioning/app_version.properties && bundle exec fastlane android release && open ./android/app/build/outputs/bundle/release;",
		"android-version": "yarn clean:android; bundle exec fastlane android version",
		"android-ci": "yarn clean:android; bundle exec fastlane android ci",
		"android-build": "yarn clean:android; bundle exec fastlane android internal",
		"reinstall": "./reinstall.sh",
		"sonar": "open -a iTerm ./sonarqube/docker-start.sh; node ./sonarqube/init.js",
		"cleanTmp": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf ios/build/ android/app/build/ android/build/ artifacts/ ios/atlantm.xcodeproj/xcuserdata/",
		"clean": "rm -rf ios/build/ ./ios/Podfile.lock ./ios/Pods ~/Library/Developer/Xcode/DerivedData/*; npx react-native-clean-project;",
		"test": "jest",
		"version": "./version.sh",
		"detox-server": "detox run-server",
		"detox:record": "detox recorder --bundleId 'com.atlantm.app' --simulatorId booted --outputTestFile './src/__tests__/test.js' --testName 'My Recorded Test' --record",
		"e2e:ios:build": "detox build -c ios.release",
		"e2e:ios": "detox test -c ios.release",
		"e2e:ios-ci": "yarn e2e:ios --workers 4 --retries 1",
		"e2e:android": "detox test -c android.emu.release",
		"e2e:android-ci-genycloud": "detox test -c android.genycloud.release --workers 5 --retries 1 --jest-report-specs --loglevel verbose",
		"e2e:android-ci-google": "detox test -c android.emu.release --workers 3 --retries 1 --jest-report-specs --loglevel verbose --headless",
		"e2e:android-debug": "detox test -c android.emu.debug",
		"bugsnag:create-build": "bugsnag-cli create-build",
		"bugsnag:upload-android-ndk": "bugsnag-cli upload android-ndk android/",
		"bugsnag:upload-android-proguard": "bugsnag-cli upload android-proguard android/",
		"bugsnag:upload-rn-android": "bugsnag-cli upload react-native-android",
		"bugsnag:upload-dsym": "bugsnag-cli upload dsym --plist ios/atlantm/Info.plist --scheme atlantm --project-root ./ios",
		"bugsnag:upload-rn-ios": "bugsnag-cli upload react-native-ios",
		"bugsnag:upload": "bugsnag-cli upload android-ndk android/ && bugsnag-cli upload android-proguard android/ && bugsnag-cli upload react-native-android && bugsnag-cli upload dsym ios/ && bugsnag-cli upload react-native-ios"
	},
