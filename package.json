{
  "name": "atlantm",
  "version": "5.0.0",
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:facebook/react-native.git"
  },
  "bin": {
    "react-native": "local-cli/wrong-react-native.js"
  },
  "scripts": {
    "android-sumulators": "emulator -list-avds",
    "ios-sumulators": "xcrun simctl list devices",
    "ios": "react-native run-ios --simulator \"iPhone X\"",
    "ios-build": "cd ios && bundle exec fastlane beta",
    "ios-release": "cd ios && bundle exec fastlane beta",
    "android": "adb uninstall com.atlantm; react-native run-android",
    "android-test": "cd android && bundle exec fastlane test && open app/build/outputs/apk",
    "android-build": "cd android && bundle exec fastlane internal",
    "android-release": "cd android && bundle exec fastlane beta",
    "reinstall": "./reinstall.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "react-devtools": "react-devtools",
    "test": "jest",
    "flow": "flow",
    "lint": "eslint .",
    "prettier": "find . -name node_modules -prune -or -name '*.js' -print | xargs prettier --write",
    "postinstall": "sed -i '' '/Google.*\\.[h|m]/d' node_modules/react-native-maps/lib/ios/AirMaps.xcodeproj/project.pbxproj; npm run clean rndebugger-open",
    "clean": "watchman watch-del-all; rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; rm -rf ios/build/; rm -rf android/build/; rm -rf ios/rbtse.xcodeproj/xcuserdata/",
    "version": "./version.sh"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "native-base": "^2.8.0",
    "pretty-format": "^4.2.1",
    "promise": "^7.1.1",
    "prop-types": "^15.6.2",
    "react-clone-referenced-element": "^1.0.1",
    "react-devtools-core": "^3.3.2",
    "react-native": "^0.56.0",
    "react-native-actionsheet": "2.4.2",
    "react-native-amplitude-analytics": "^0.1.18",
    "react-native-animatable": "^1.3.0",
    "react-native-communications": "2.2.1",
    "react-native-datepicker": "1.7.2",
    "react-native-device-info": "0.22.5",
    "react-native-devsettings-android": "0.1.5",
    "react-native-fcm": "16.2.0",
    "react-native-fetch-blob": "0.10.8",
    "react-native-google-analytics-bridge": "6.0.1",
    "react-native-image-crop-picker": "0.21.1",
    "react-native-image-gallery": "*",
    "react-native-loading-spinner-overlay": "0.5.2",
    "react-native-maps": "0.21.0",
    "react-native-sentry": "0.39.0",
    "react-native-splash-screen": "3.1.1",
    "react-native-swiper": "1.5.13",
    "react-native-vector-icons": "5.0.0",
    "react-navigation": "2.12.1",
    "react-redux": "5.0.7",
    "react-timer-mixin": "^0.13.4",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "2.3.0",
    "uglifyjs": "2.4.11",
    "xcode": "^0.9.1"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-preset-react-native": "^5.0.1",
    "eslint": "5.4.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-native": "3.2.1",
    "eslint-config-fb-strict": "^20.0.3",
    "eslint-config-fbjs": "^1.1.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-prettier": "2.1.1",
    "flow-bin": "^0.58.0",
    "jest": "23.5.0",
    "prettier": "1.7.0",
    "react": "^16.4.2",
    "react-devtools": "3.3.2",
    "react-native-debugger-open": "0.3.17",
    "react-test-renderer": "16.4.2",
    "redux-logger": "3.0.6"
  },
  "peerDependencies": {
    "react": "16.0.0"
  },
  "prettier": {
    "requirePragma": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": true,
    "parser": "flow"
  }
}
