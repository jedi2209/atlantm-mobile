{
  "name": "Atlant-M",
  "version": "8.4.5",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "yarn android-build; yarn ios-build;",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "npx patch-package; cd ios; pod update; cd ../; npx pod-install;",
    "react": "watchman watch-del $(pwd); watchman watch-project $(pwd); npx react-native start --reset-cache",
    "check-dependencies": "rnx-dep-check",
    "fix-dependencies": "rnx-dep-check --write",
    "iphone": "yarn react-native run-ios --simulator=\"iPhone 14\"",
    "iphonePro": "yarn react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "iphoneProMax": "yarn react-native run-ios --simulator=\"iPhone 14 Pro Max\"",
    "ios": "yarn iphoneProMax",
    "ios-build": "bundle exec fastlane ios beta; rm atlantm.app.dSYM.zip atlantm.ipa;",
    "android": "cd android && ./gradlew clean; cd ../; open -a iTerm ./android/dev.sh && adb wait-for-device && sleep 10; adb uninstall com.atlantm; sleep 10; npx react-native run-android --active-arch-only",
    "android-local": "bundle exec fastlane android test && adb uninstall com.atlantm; adb install ./android/app/build/outputs/bundle/release/app-release.aab && adb shell am start -n com.atlantm/.MainActivity",
    "android-release": "git checkout ./android/app/versioning/app_version.properties && bundle exec fastlane android release && open ./android/app/build/outputs/bundle/release;",
    "android-build": "bundle exec fastlane android internal",
    "reinstall": "./reinstall.sh",
    "sonar": "open -a iTerm ./sonarqube/docker-start.sh; node ./sonarqube/init.js",
    "cleanTmp": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf ios/build/ android/app/build/ android/build/ artifacts/ ios/atlantm.xcodeproj/xcuserdata/",
    "clean": "npx react-native clean-project-auto; rm -rf ./ios/Podfile.lock; rm -rf ~/Library/Developer/Xcode/DerivedData/*;",
    "test": "jest",
    "version": "./version.sh",
    "detox-server": "detox run-server",
    "detox:record": "detox recorder --bundleId 'com.atlantm.app' --simulatorId booted --outputTestFile './src/__tests__/test.js' --testName 'My Recorded Test' --record",
    "e2e:ios:build": "detox build -c ios.sim.release",
    "e2e:ios": "detox test -c ios.sim.release",
    "e2e:ios-ci": "yarn e2e:ios -- --workers 4 --retries 1 --jest-report-specs -l verbose",
    "e2e:android": "detox test -c android.emu.release",
    "e2e:android-ci-genycloud": "detox test -c android.genycloud.release --workers 5 --retries 1 --jest-report-specs --loglevel verbose",
    "e2e:android-ci-google": "detox test -c android.emu.release --workers 3 --retries 1 --jest-report-specs --loglevel verbose --headless",
    "e2e:android-debug": "detox test -c android.emu.debug"
  },
