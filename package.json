{
  "name": "Atlant-M",
  "version": "5.1.4",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "iphone": "react-native run-ios --simulator=\"iPhone 8\"",
    "iphonex": "react-native run-ios --simulator=\"iPhone X\"",
    "iphonexr": "react-native run-ios --simulator=\"iPhone XÊ€\"",
    "react": "react-native start --reset-cache",
    "ios-build": "bundle exec fastlane ios beta",
    "android": "adb uninstall com.atlantm; react-native run-android",
    "android-test": "bundle exec fastlane android test && adb uninstall com.atlantm; adb install android/app/build/outputs/apk/release/app-universal-release.apk; adb shell am start -n com.atlantm/.MainActivity",
    "android-release": "git checkout ./android/app/versioning/app_version.properties && bundle exec fastlane android release && open ./android/app/build/outputs/apk/release;",
    "android-build": "bundle exec fastlane android internal",
    "reinstall": "./reinstall.sh",
    "clean": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all; rm -rf ios/build/; rm -rf android/build/; rm -rf ios/rbtse.xcodeproj/xcuserdata/",
    "version": "./version.sh"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.4.2",
    "acorn": "^6.2.0",
    "html-entities": "^1.2.1",
    "lodash": "4.17.11",
    "moment": "2.22.1",
    "prop-types": "15.6.1",
    "react": "16.8.3",
    "react-native": "0.59",
    "react-native-actionsheet": "2.4.2",
    "react-native-amplitude-analytics": "0.2.3",
    "react-native-animatable": "1.3.0",
    "react-native-communications": "2.2.1",
    "react-native-datepicker": "1.7.2",
    "react-native-device-info": "0.24.3",
    "react-native-devsettings-android": "0.2.1",
    "react-native-gallery-swiper": "1.22.2",
    "react-native-gesture-handler": "1.3.0",
    "react-native-image-crop-picker": "0.21.2",
    "react-native-image-gallery": "2.1.5",
    "react-native-loading-spinner-overlay": "1.0.1",
    "react-native-maps": "github:react-native-community/react-native-maps#master",
    "react-native-onesignal": "3.2.12",
    "react-native-rate": "1.1.6",
    "react-native-sentry": "^0.39.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-swiper": "1.5.14",
    "react-navigation": "1.6.1",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-form": "7.3.0",
    "redux-persist": "4.10.2",
    "redux-thunk": "2.3.0",
    "rn-fetch-blob": "0.10.13"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-check-constants": "7.0.0-beta.38",
    "babel-eslint": "^10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-module-resolver": "3.1.1",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.14.2",
    "jest": "24.8.0",
    "metro-react-native-babel-preset": "0.51.0",
    "native-base": "2.12.1",
    "react-test-renderer": "16.5.0",
    "redux-logger": "3.0.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
