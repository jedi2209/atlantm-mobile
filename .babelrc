{
  "env": {
    "production": {
      "presets": ["module:metro-react-native-babel-preset"],
      "plugins": [
        ["@babel/check-constants"],
        ["module-resolver", {
          "cwd": "babelrc",
          "root": ["./src"],
          "alias": {
            "@catalog": "./src/catalog",
            "@contacts": "./src/contacts",
            "@core": "./src/core",
            "@dealer": "./src/dealer",
            "@eko": "./src/eko",
            "@indicators": "./src/indicators",
            "@info": "./src/info",
            "@intro": "./src/intro",
            "@menu": "./src/menu",
            "@navigation": "./src/navigation",
            "@profile": "./src/profile",
            "@services": "./src/services",
            "@tva": "./src/tva",
            "@utils": "./src/utils"
          }
        }],
        "react-native-reanimated/plugin",
        ["transform-remove-console", {"exclude": ["error", "warn"] }]
      ]
    },
    "development": {
      "presets": ["module:metro-react-native-babel-preset"],
      "plugins": [
        ["@babel/check-constants"],
        ["module-resolver", {
          "cwd": "babelrc",
          "root": ["./src"],
          "alias": {
            "@catalog": "./src/catalog",
            "@contacts": "./src/contacts",
            "@core": "./src/core",
            "@dealer": "./src/dealer",
            "@eko": "./src/eko",
            "@indicators": "./src/indicators",
            "@info": "./src/info",
            "@intro": "./src/intro",
            "@menu": "./src/menu",
            "@navigation": "./src/navigation",
            "@profile": "./src/profile",
            "@services": "./src/services",
            "@tva": "./src/tva",
            "@utils": "./src/utils"
          }
        }],
        "react-native-reanimated/plugin"
      ]
    }
  }
}
